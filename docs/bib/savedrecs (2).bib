Title = {{Spatial distribution of Antarctic minke whales (Balaenoptera
   bonaerensis) in relation to spatial distributions of krill in the Ross
   Sea, Antarctica}},
Journal = {{FISHERIES OCEANOGRAPHY}},
Year = {{2013}},
Volume = {{22}},
