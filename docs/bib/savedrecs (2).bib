@article{ ISI:000310834900001,
Author = {Diamond, Sarah E. and Nichols, Lauren M. and McCoy, Neil and Hirsch,
   Christopher and Pelini, Shannon L. and Sanders, Nathan J. and Ellison,
   Aaron M. and Gotelli, Nicholas J. and Dunn, Robert R.},
Title = {{A physiological trait-based approach to predicting the responses of
   species to experimental climate warming}},
Journal = {{ECOLOGY}},
Year = {{2012}},
Volume = {{93}},
Number = {{11}},
Pages = {{2305-2312}},
Month = {{NOV}},
Abstract = {{Physiological tolerance of environmental conditions can influence
   species-level responses to climate change. Here, we used
   species-specific thermal tolerances to predict the community responses
   of ant species to experimental forest-floor warming at the northern and
   southern boundaries of temperate hardwood forests in eastern North
   America. We then compared the predictive ability of thermal tolerance
   vs. correlative species distribution models (SDMs) which are popular
   forecasting tools for modeling the effects of climate change. Thermal
   tolerances predicted the responses of 19 ant species to experimental
   climate warming at the southern site, where environmental conditions are
   relatively close to the ants' upper thermal limits. In contrast, thermal
   tolerances did not predict the responses of the six species in the
   northern site, where environmental conditions are relatively far from
   the ants' upper thermal limits. Correlative SDMs were not predictive at
   either site. Our results suggest that, in environments close to a
   species' physiological limits, physiological trait-based measurements
   can successfully forecast the responses of species to future conditions.
   Although correlative SDMs may predict large-scale responses, such models
   may not be accurate for predicting sitelevel responses.}},
